{
  "name": "jmaps",
  "version": "0.4.1",
  "description": "A light Javascript API for reading Natural Earth DB files",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover -x ./test/main.js _mocha -- -R spec ./test/main.js",
    "check-coverage": "istanbul check-coverage --statements 99 --branches 90 --functions 100 --lines 99",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "npm run test && npm run check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/jmaps.git"
  },
  "keywords": [
    "maps",
    "natural earth",
    "shapefile",
    "dbf",
    "shp",
    "geojson"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/jmaps/issues"
  },
  "homepage": "https://github.com/jclo/jmaps",
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "husky": "0.14.3",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "opener": "1.4.3"
  }
}
